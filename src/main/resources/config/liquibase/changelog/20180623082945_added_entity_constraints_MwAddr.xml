        <addForeignKeyConstraint baseColumnNames="mw_cmnty_id"
                                 baseTableName="mw_addr"
                                 constraintName="fk_mw_addr_mw_cmnty_id"
                                 referencedColumnNames="id"
                                 referencedTableName="mw_cmnty"/>

    </changeSet>
</databaseChangeLog>
